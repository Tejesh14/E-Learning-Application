@{
}
<h1 style="color: #FF9F1C" class="display-4 mt-3">Projects</h1>

<div>
    <form method="post" action="Create">
        <label class="text-light">Select Student</label><br />
        <select name="student" id="cars">
            <option selected>Select</option>
        @foreach (var item in ViewBag.Students)
        {
            <option value="@item" class="text-dark">@item</option>
        }
        </select>
        <br />
        <br />
        <button type="submit" class="btn btn-sm" style="background-color: #FF9F1C">Submit</button>
    </form>
</div>